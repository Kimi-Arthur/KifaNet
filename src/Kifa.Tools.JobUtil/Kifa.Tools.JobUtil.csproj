    <TargetFramework>net6.0</TargetFramework>
    <ToolCommandName>jobutil</ToolCommandName>
    <PackAsTool>true</PackAsTool>
    <Version>2.0.1</Version>
    <PackageLicenseUrl>https://raw.githubusercontent.com/Kimi-Arthur/Pimix/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Kimi-Arthur/KifaNet</PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/Kimi-Arthur/Pimix/master/Pimix.png</PackageIconUrl>
    <Description>Utility to manage pimix jobs.</Description>
    <Copyright>Copyright © 2015-2018 Kimi Arthur</Copyright>
    <PackageTags>Pimix Apps JobUtil app</PackageTags>
    <Authors>Kimi Arthur</Authors>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.6.0"/>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Kifa.Configs\Kifa.Configs.csproj"/>
    <ProjectReference Include="..\Kifa.Service\Kifa.Service.csproj"/>
  </ItemGroup>
</Project>