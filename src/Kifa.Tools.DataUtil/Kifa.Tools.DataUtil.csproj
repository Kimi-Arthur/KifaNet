<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <ToolCommandName>datax</ToolCommandName>
    <PackAsTool>true</PackAsTool>
    <Version>6.1.5</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Kimi-Arthur/KifaNet</PackageProjectUrl>
    <PackageIcon>Kifa.png</PackageIcon>
    <Description>Utility to manage data.</Description>
    <Copyright>Copyright Â© 2015-2022 Kimi-Arthur</Copyright>
    <PackageTags>Kifa DataUtil datax</PackageTags>
    <Authors>KimiArthur</Authors>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Kifa.Apps.MomentCounter\Kifa.Apps.MomentCounter.csproj" />
    <ProjectReference Include="..\Kifa.Bilibili\Kifa.Bilibili.csproj" />
    <ProjectReference Include="..\Kifa.Cloud.Telegram\Kifa.Cloud.Telegram.csproj" />
    <ProjectReference Include="..\Kifa.Infos\Kifa.Infos.csproj" />
    <ProjectReference Include="..\Kifa.Memrise\Kifa.Memrise.csproj" />
    <ProjectReference Include="..\Kifa.Music\Kifa.Music.csproj" />
    <ProjectReference Include="..\Kifa.Tools\Kifa.Tools.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\resources\Kifa.png" Pack="true" PackagePath="\" />
  </ItemGroup>
</Project>
