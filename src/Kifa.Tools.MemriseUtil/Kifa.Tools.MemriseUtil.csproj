<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <ToolCommandName>memx</ToolCommandName>
    <PackAsTool>true</PackAsTool>
    <Version>3.1.3</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Kimi-Arthur/KifaNet</PackageProjectUrl>
    <PackageIcon>Kifa.png</PackageIcon>
    <Description>Utility to import Memrise words.</Description>
    <Copyright>Copyright Â© 2021-2022 Kimi-Arthur</Copyright>
    <PackageTags>Kifa MemriseUtil memx</PackageTags>
    <Authors>KimiArthur</Authors>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Kifa.Languages\Kifa.Languages.csproj"/>
    <ProjectReference Include="..\Kifa.Memrise\Kifa.Memrise.csproj"/>
    <ProjectReference Include="..\Kifa.Tools.DataUtil\Kifa.Tools.DataUtil.csproj"/>
    <ProjectReference Include="..\Kifa.Tools\Kifa.Tools.csproj"/>
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\resources\Kifa.png" Pack="true" PackagePath="\"/>
  </ItemGroup>
</Project>
