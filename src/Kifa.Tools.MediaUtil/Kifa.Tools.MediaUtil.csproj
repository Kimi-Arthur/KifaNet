<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <ToolCommandName>mediax</ToolCommandName>
    <PackAsTool>true</PackAsTool>
    <Version>4.0.3</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Kimi-Arthur/KifaNet</PackageProjectUrl>
    <PackageIcon>Kifa.png</PackageIcon>
    <Description>Utility to manage media files.</Description>
    <Copyright>Copyright Â© 2022 Kimi-Arthur</Copyright>
    <PackageTags>Kifa MediaUtil mediax</PackageTags>
    <Authors>Kimi-Arthur</Authors>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Kifa.Bilibili\Kifa.Bilibili.csproj"/>
    <ProjectReference Include="..\Kifa.Graphics\Kifa.Graphics.csproj"/>
    <ProjectReference Include="..\Kifa.Tools\Kifa.Tools.csproj"/>
    <ProjectReference Include="..\Kifa\Kifa.csproj"/>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.9.1"/>
    <PackageReference Include="FFMpegCore" Version="4.8.0"/>
    <PackageReference Include="SixLabors.ImageSharp" Version="2.1.3"/>
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\resources\Kifa.png" Pack="true" PackagePath=""/>
  </ItemGroup>
</Project>
