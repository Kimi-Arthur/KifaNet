<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net8.0</TargetFramework>
        <ToolCommandName>skyx</ToolCommandName>
        <PackAsTool>true</PackAsTool>
        <Version>4.0.5</Version>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/Kimi-Arthur/KifaNet</PackageProjectUrl>
        <PackageIcon>Kifa.png</PackageIcon>
        <Description>Utility to download from sky.ch.</Description>
        <Copyright>Copyright Â© 2022 Kimi-Arthur</Copyright>
        <PackageTags>Kifa SkyChDownloader skyx</PackageTags>
        <Authors>Kimi-Arthur</Authors>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="CommandLineParser" Version="2.9.1"/>
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\..\Kifa.Jobs\Kifa.Jobs.csproj"/>
        <ProjectReference Include="..\Kifa.Api.Files\Kifa.Api.Files.csproj"/>
        <ProjectReference Include="..\Kifa.IO\Kifa.IO.csproj"/>
        <ProjectReference Include="..\Kifa.Media.MpegDash\Kifa.Media.MpegDash.csproj"/>
        <ProjectReference Include="..\Kifa.Service\Kifa.Service.csproj"/>
        <ProjectReference Include="..\Kifa.SkyCh\Kifa.SkyCh.csproj"/>
        <ProjectReference Include="..\Kifa.Tools\Kifa.Tools.csproj"/>
        <ProjectReference Include="..\Kifa\Kifa.csproj"/>
    </ItemGroup>
    <ItemGroup>
        <None Include="..\..\resources\Kifa.png" Pack="true" PackagePath="\"/>
    </ItemGroup>
</Project>
