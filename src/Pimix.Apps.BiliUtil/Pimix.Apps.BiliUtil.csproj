<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <ToolCommandName>bilix</ToolCommandName>
    <PackAsTool>true</PackAsTool>
    <Version>4.1.9</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Kimi-Arthur/Pimix</PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/Kimi-Arthur/Pimix/master/Pimix.png</PackageIconUrl>
    <Description>Utility to interact with bilibili.</Description>
    <Copyright>Copyright © 2019 Kimi Arthur</Copyright>
    <PackageTags>Pimix Apps BiliUtil app</PackageTags>
    <Authors>Kimi Arthur</Authors>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Pimix.Api.Files\Pimix.Api.Files.csproj" />
    <ProjectReference Include="..\Pimix.Apps\Pimix.Apps.csproj" />
    <ProjectReference Include="..\Pimix.Bilibili\Pimix.Bilibili.csproj" />
    <ProjectReference Include="..\Pimix.Configs\Pimix.Configs.csproj" />
    <ProjectReference Include="..\Pimix.Service\Pimix.Service.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.4.3" />
    <PackageReference Include="NLog.Schema" Version="4.5.10" />
  </ItemGroup>
  <ItemGroup>
    <None Update="NLog.config">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
